{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "GatewayDb": ""
  },
  "Authentication": {
    "Authority": "",
    "PublicAuthority": "",
    "ClientId": "",
    "ClientSecret": "",
    "Audience": "",
    "RedirectUri": "",
    "Realm": ""
  },
  "Keycloak": {
    "AdminClientId": "",
    "AdminClientSecret": ""
  },
  "Frontend": {
    "Url": "http://localhost:3000"
  },
  "RabbitMQ": {
    "Host": "",
    "Username": "",
    "Password": ""
  },
  "Jaeger": {
    "Host": "",
    "Port": ""
  },
  "RateLimit": {
    "MaxAttempts": 5,
    "WindowMinutes": 1
  },
  "ReverseProxy": {
    "Routes": {
      "journey-public-route": {
        "ClusterId": "journey-cluster",
        "AuthorizationPolicy": null,
        "Match": {
          "Path": "/api/journeys/public/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/journeys/public/{**catch-all}"
          }
        ]
      },
      "journey-route": {
        "ClusterId": "journey-cluster",
        "Match": {
          "Path": "/api/journeys/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/journeys/{**catch-all}"
          }
        ]
      },
      "notification-route": {
        "ClusterId": "notification-cluster",
        "Match": {
          "Path": "/api/notifications/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/notifications/{**catch-all}"
          }
        ]
      },
      "notification-hub-route": {
        "ClusterId": "notification-cluster",
        "Match": {
          "Path": "/hubs/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/hubs/{**catch-all}"
          }
        ]
      },
      "admin-journey-route": {
        "ClusterId": "journey-cluster",
        "AuthorizationPolicy": "AdminOnly",
        "Match": {
          "Path": "/api/admin/journeys/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/admin/journeys/{**catch-all}"
          }
        ]
      },
      "admin-statistics-route": {
        "ClusterId": "journey-cluster",
        "AuthorizationPolicy": "AdminOnly",
        "Match": {
          "Path": "/api/admin/statistics/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/admin/statistics/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "journey-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://journey-api"
          }
        }
      },
      "notification-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://notification-api"
          }
        }
      }
    }
  }
}
